                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Set 2"
                android:layout_gravity="left"
                android:textSize="15sp"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Set 3"
                android:layout_gravity="left"
                android:textSize="15sp"
                />


            <!--            Row 2-->
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Incline Bench"
                android:layout_gravity="left"
                android:textSize="15sp"
                />
            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="text"
                android:text="150"
                />

            <com.example.gymassistant.SetButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:maxNum="5"
                android:layout_gravity="center_horizontal"
                />
            <com.example.gymassistant.SetButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:maxNum="6"
                android:layout_gravity="center_horizontal"
                />
            <com.example.gymassistant.SetButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:maxNum="8"
                android:layout_gravity="center_horizontal"
                />


            <!--            Row 3-->
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
